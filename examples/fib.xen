// Various methods of computing the fibonnacci sequence

println("Naive method:");
fn fib_naive(n) {
    var a = 0;
    var b = 1;
    var next = b;
    var count = 1;

    while (count <= n) {
        println(next);
        count++;
        a = b;
        b = next;
        next = a + b;
    }
}

fib_naive(10);

println("Recursion:");
fn fib_recurse(n) {
    if (n < 0) {
        println("Incorrect input");
    } else if (n == 0) {
        return 0;
    } else if (n == 1 or n == 2) {
        return 1;
    } else {
        return fib_recurse(n - 1) + fib_recurse(n - 2);
    }
}

println(fib_recurse(10)); // should print 55
