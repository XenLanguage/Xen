include io;


class Counter {
    private count = 0;

    init(count) {
        this.count = count;
    }

    fn increment() {
        this.count = this.count + 1;
    }

    fn get() {
        return this.count;
    }
};

var c = new Counter(10);
c.increment();
c.increment();
io.println(c.get());
